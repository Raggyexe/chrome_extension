{
	"name": "Snake",
	"version": "1.1.0",
	"manifest_version":2,
	"description": "The best chrome popup Snake game!",
	"content_scripts": [
		{
	  		"matches": ["<all_urls>"],
	  		"js": ["script.js"],
	  		"run_at": "document_end",
	  		"all_frames": true
		}
	],
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"minimum_chrome_version": "16.0.0.0"
}
